adcFlushFifo();
// Disable the ADC
adcDisable();

// Enable the ADC
fwDelayUs(850, FW_DELAY_RANGE_1_MS);


//Select CHAN 3
gpioSetOutput(AUXIO_O_S0);
gpioSetOutput(AUXIO_O_S1);
gpioClearOutput(AUXIO_O_S2);
gpioClearOutput(AUXIO_O_S3);
adcSelectGpioInput(AUXIO_A_ADC_INPUT0);
adcEnableSync(ADC_REF_FIXED, ADC_SAMPLE_TIME_10P9_MS, ADC_TRIGGER_MANUAL);

// Sample the analog sensor
adcGenManualTrigger();
adcReadFifo(output.adcVal3);
adcFlushFifo();
// Disable the ADC
adcDisable();


// Enable the ADC
fwDelayUs(850, FW_DELAY_RANGE_1_MS);

//Select CHAN 4
gpioClearOutput(AUXIO_O_S0);
gpioClearOutput(AUXIO_O_S1);
gpioSetOutput(AUXIO_O_S2);
gpioClearOutput(AUXIO_O_S3);
adcSelectGpioInput(AUXIO_A_ADC_INPUT0);
adcEnableSync(ADC_REF_FIXED, ADC_SAMPLE_TIME_10P9_MS, ADC_TRIGGER_MANUAL);

// Sample the analog sensor
adcGenManualTrigger();
adcReadFifo(output.adcVal4);
adcFlushFifo();
// Disable the ADC
adcDisable();


// Enable the ADC
fwDelayUs(850, FW_DELAY_RANGE_1_MS);


//Select CHAN 5
gpioSetOutput(AUXIO_O_S0);
gpioClearOutput(AUXIO_O_S1);
gpioSetOutput(AUXIO_O_S2);
gpioClearOutput(AUXIO_O_S3);
adcSelectGpioInput(AUXIO_A_ADC_INPUT0);
adcEnableSync(ADC_REF_FIXED, ADC_SAMPLE_TIME_10P9_MS, ADC_TRIGGER_MANUAL);

// Sample the analog sensor
adcGenManualTrigger();
adcReadFifo(output.adcVal5);
adcFlushFifo();
// Disable the ADC
adcDisable();

// Enable the ADC
fwDelayUs(850, FW_DELAY_RANGE_1_MS);

//Select CHAN 6
gpioClearOutput(AUXIO_O_S0);
gpioSetOutput(AUXIO_O_S1);
gpioSetOutput(AUXIO_O_S2);
gpioClearOutput(AUXIO_O_S3);
adcSelectGpioInput(AUXIO_A_ADC_INPUT0);
adcEnableSync(ADC_REF_FIXED, ADC_SAMPLE_TIME_10P9_MS, ADC_TRIGGER_MANUAL);

// Sample the analog sensor
adcGenManualTrigger();
adcReadFifo(output.adcVal6);
adcFlushFifo();
// Disable the ADC
adcDisable();


// Enable the ADC
fwDelayUs(850, FW_DELAY_RANGE_1_MS);

//Select CHAN 7
gpioSetOutput(AUXIO_O_S0);
gpioSetOutput(AUXIO_O_S1);
gpioSetOutput(AUXIO_O_S2);
gpioClearOutput(AUXIO_O_S3);
adcSelectGpioInput(AUXIO_A_ADC_INPUT0);
adcEnableSync(ADC_REF_FIXED, ADC_SAMPLE_TIME_10P9_MS, ADC_TRIGGER_MANUAL);

// Sample the analog sensor
adcGenManualTrigger();
adcReadFifo(output.adcVal7);
adcFlushFifo();
// Disable the ADC
adcDisable();


// Enable the ADC
fwDelayUs(850, FW_DELAY_RANGE_1_MS);


//Select CHAN 8
gpioClearOutput(AUXIO_O_S0);
gpioClearOutput(AUXIO_O_S1);
